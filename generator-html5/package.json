{
  "name": "main-site",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint ./lib ./static",
    "test": "mocha test --recursive",
    "link": "cd node_modules && ln -f -s ../../../uc/uc-proto && ln -f -s ../../../article/article-proto && ln -f -s ../../../banner/banner-proto  && ln -f -s ../../../file/file-proto   && ln -f -s ../../../activity/activity-proto   && ln -f -s ../../../atlas/atlas-proto",
    "webpack": "NODE_ENV=production node tools/fix-babel-runtime.js && ./node_modules/.bin/webpack -d --progress --colors",
    "start": "./node_modules/.bin/nodemon -w src -x ./node_modules/.bin/babel-node index",
    "dev:server": "NODE_ENV=dev UV_THREADPOOL_SIZE=10 nodemon -w lib -x babel-node tools/dev",
    "dev:webpack": "node tools/fix-babel-runtime.js && ./node_modules/.bin/webpack-dev-server -d --progress --colors",
    "dev:compass": "compass compile",
    "dev:mobilecompass": "compass compile -c config.mobile.rb",
    "dev:compass watch": "compass watch",
    "dev:browser-sync": "./node_modules/.bin/browser-sync start --proxy localhost:5001 --port 3001 -f static/style/style",
    "mobiledev:browser-sync": "./node_modules/.bin/browser-sync start --proxy localhost:5101 --port 3101 -f static/style/style",
    "appdev:browser-sync": "./node_modules/.bin/browser-sync start --proxy localhost:5201 --port 3201 -f static/style/style",
    "dev": "NODE_ENV=dev platform=pc ./node_modules/.bin/concurrently './node_modules/.bin/spritesmith' 'npm run dev:server' 'npm run dev:webpack' 'npm run dev:browser-sync'",
    "cp:images": "cp -r static/pc_site/images/ dist/static/pc_site/images",
    "cp:mobileimages": "cp -r static/mobile_site/images/ dist/static/mobile_site/images",
    "production": "NODE_ENV=production platform=pc ./node_modules/.bin/concurrently 'npm run cp:images' './node_modules/.bin/spritesmith' 'npm run dev:server' 'npm run webpack' 'npm run dev:compass'",
    "mobile:production": "NODE_ENV=production platform=mobile ./node_modules/.bin/concurrently 'npm run cp:mobileimages' './node_modules/.bin/spritesmith' 'npm run dev:server' 'npm run webpack' 'npm run dev:mobilecompass'",
    "mobiledev": "NODE_ENV=dev platform=mobile ./node_modules/.bin/concurrently './node_modules/.bin/spritesmith' 'npm run dev:server' 'npm run dev:webpack' 'npm run mobiledev:browser-sync' 'npm run dev:mobilecompass' 'npm run dev:compass watch'",
    "appdev": "NODE_ENV=dev platform=api ./node_modules/.bin/concurrently 'npm run dev:server'",
    "build0": "./node_modules/.bin/concurrently 'node tools/fix-babel-runtime.js && ./node_modules/.bin/webpack' './node_modules/.bin/copyfiles -u 2 static/uc/*.html dist/static/uc/'"
  },
  "pre-commit": [
    "eslint"
  ],
  "author": "",
  "license": "ISC",
